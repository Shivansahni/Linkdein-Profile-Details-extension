{
  "name": "Linkedin profile details",
  "version": "0.0.1",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "logo.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.linkedin.com/*"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "128": "logo.png"
  },
  "permissions": ["activeTab", "Tabs"]
}
